#!/bin/bash
sbcnname=$KEYVAULT_0_SUBSCRIPTION
kvname=$KEYVAULT_0_NAME

az keyvault secret delete  \
  --subscription "$sbcnname" \
  --vault-name "$kvname" \
  --name "$name"

az keyvault secret purge  \
  --subscription "$sbcnname" \
  --vault-name "$kvname" \
  --name "$name"

# az keyvault secret purge  \
#   --subscription "$sbcnname" \
#   --vault-name "$kvname" \
#   --name "$name"
