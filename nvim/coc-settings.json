{
    "diagnostic.errorSign": "✖",
    "diagnostic.warningSign": "⚠",
    "diagnostic.infoSign": "ℹ",
    "diagnostic.hintSign": "➤",
    "suggest.noselect": false,
    "suggest.echodocSupport": true,
    "codeLens.enable": true,
    "signature.enable": true,
    "suggest.preferCompleteThanJumpPlaceholder": true,
    "typescript.preferences.quoteStyle": "single",
    "typescript.preferences.importModuleSpecifier": "auto",
    "tsserver.implicitProjectConfig.experimentalDecorators": true,
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "html.suggest.html5": true,
    "python.trace.server": "verbose",
    "python.venvFolders": ["envs",".pyenv",".direnv", "~/.local/share/virtualenvs", "~/.cache/pypoetry/virtualenvs"],
    "python.workspaceSymbols.exclusionPatterns": [],
    "python.jediEnabled": false,
    "python.jediPath": "~/.pyenv/versions/3.8.0/envs/neovim3/lib/python3.8/site-packages",
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": ["--load-plugins", "pylint_django"],
    "python.linting.pylintPath": "~/.pyenv/versions/neovim3/bin/pylint",
    "python.linting.mypyEnabled": true,
    "python.linting.mypyPath": "~/.pyenv/versions/neovim3/bin/mypy",
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "~/.pyenv/versions/neovim3/bin/black",
    "python.sortImports.path": "~/.pyenv/versions/neovim3/bin/isort",
    "git.addedSign.text": "▎",
    "git.changedSign.text": "▎",
    "git.removedSign.text": "▎",
    "git.changeRemovedSign.text": "▶",
    "git.topRemovedSign.text": "◥",
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "explorer.icon.enableNerdfont": true,
    "explorer.width": 40,
    "yaml.schemas": {
        "https://raw.githubusercontent.com/microsoft/azure-pipelines-vscode/main/service-schema.json": ["*/ci/pipelines/*.yml"]
    },
    "explorer.quitOnOpen": true
}
